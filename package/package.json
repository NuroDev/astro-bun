{
  "name": "@nurodev/astro-bun",
  "description": "An Astro adapter to deploy your SSR site to Bun targets",
  "version": "1.1.3",
  "author": {
    "email": "ben@nuro.dev",
    "name": "Ben Dixon",
    "url": "https://nuro.dev"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": ["astro-adapter", "astro-integration", "withastro", "astro", "bun"],
  "homepage": "https://github.com/nurodev/astro-bun",
  "files": ["dist"],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server.js": {
      "default": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "astro-integration-kit": "^0.16.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "astro": "^4.12.2"
  },
  "peerDependenciesMeta": {
    "astro": {
      "optional": false
    }
  }
}
